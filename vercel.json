{
  "version": 2,
  "buildCommand": "node vercel-build.cjs",
  "outputDirectory": "dist",
  "routes": [
    {
      "src": "/_vercel/insights/script.js",
      "dest": "/vercel-analytics-mock.js",
      "headers": { 
        "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    },
    {
      "src": "/_vercel/insights/(.*)",
      "dest": "/vercel-analytics-mock.js",
      "headers": { 
        "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "pragma": "no-cache",
        "expires": "0"
      }
    },
    {
      "src": "/vercel-analytics-mock.js",
      "headers": { 
        "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "pragma": "no-cache",
        "expires": "0",
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/noop.js",
      "headers": { 
        "cache-control": "no-store, no-cache, must-revalidate, proxy-revalidate",
        "pragma": "no-cache",
        "expires": "0",
        "content-type": "application/javascript"
      },
      "continue": true
    },
    {
      "src": "/assets/(.*)",
      "headers": { "cache-control": "public, max-age=31536000, immutable" },
      "continue": true
    },
    { 
      "src": "/(.*)\\.js$",
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    { 
      "src": "/(.*)\\.mjs$",
      "headers": { "content-type": "application/javascript" },
      "continue": true
    },
    { 
      "src": "/(.*)\\.css$",
      "headers": { "content-type": "text/css" },
      "continue": true
    },
    {
      "src": "/manifest.json",
      "headers": { "content-type": "application/json" },
      "continue": true
    },
    {
      "src": "/service-worker.js",
      "headers": { "cache-control": "public, max-age=0, must-revalidate" },
      "continue": true
    },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/_vercel/insights/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
        },
        {
          "key": "Pragma",
          "value": "no-cache"
        },
        {
          "key": "Expires",
          "value": "0"
        }
      ]
    }
  ]
}